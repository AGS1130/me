{
  "buildOptions": {
    "clean": true
  },
  "devOptions": {
    "hmrDelay": 300
  },
  "mount": {
    "_output": "/",
    "src": "/_dist_"
  },
  "plugins": [
    ["@snowpack/plugin-dotenv"],
    [
      "@snowpack/plugin-run-script",
      { "cmd": "eleventy", "watch": "$1 --watch" }
    ]
  ]
}
